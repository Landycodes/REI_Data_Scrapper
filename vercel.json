{
    "version": 2,
    "builds": [
      { "src": "client/package.json", "use": "@vercel/static-build", "outputDirectory": "client/build" },
      { "src": "server/*", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/scrap", "dest": "server/server.js" },
      { "src": "/(.*)", "dest": "client/build/$1" }
    ]
  }
  