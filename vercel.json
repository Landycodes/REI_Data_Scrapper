{
    "version": 2,
    "outputDirectory": "build/client",
    "builds": [
      { "src": "client/package.json", "use": "@vercel/static-build"},
      { "src": "server/*", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/scrap", "dest": "server/server.js" },
      { "src": "/(.*)", "dest": "client/*" }
    ]
  }
  